@model TipMvcApp.Models.TicketPriority

@{
    ViewData["Title"] = "Edit";
}
<link rel="stylesheet" href="~/css/StyleSheet.css" />
<h1>Edit</h1>

<h4>TicketPriority</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="PriorityId" class="control-label"></label>
                <input asp-for="PriorityId" readonly class="form-control" />
                <span asp-validation-for="PriorityId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="PriorityName" class="control-label"></label>
                <input asp-for="PriorityName" class="form-control" />
                <span asp-validation-for="PriorityName" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="RespondWithin" class="control-label"></label>
                <input asp-for="RespondWithin" class="form-control" />
                <span asp-validation-for="RespondWithin" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="ResolveWithin" class="control-label"></label>
                <input asp-for="ResolveWithin" class="form-control" />
                <span asp-validation-for="ResolveWithin" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" /> |
                <a type="submit" class="btn btn-outline-primary" asp-action="Index">Back to List</a>
            </div>
        </form>
    </div>
</div>


@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
